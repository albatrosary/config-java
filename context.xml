<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>
<Context>
    <Resource name="jdbc/arms"
              auth="Container"
              type="javax.sql.DataSource"
              driverClassName="com.mysql.jdbc.Driver"
              url="jdbc:mysql://localhost:3306/arms?useSSL=false&amp;useUnicode=true&amp;characterEncoding=utf8&amp;characterSetResults=utf8"
              username="armsuser"
              password="armspass"
              maxActive="30"
              maxIdle="10"
              maxWait="5000" />
</Context>
